{
  "name": "rafi-api-v2",
  "version": "1.0.0",
  "description": "This project was bootstrapped with Fastify-CLI.",
  "main": "app.ts",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "test": "yarn build:ts && tsc -p test/tsconfig.json && tap --jobs=1 --ts \"test/**/*.test.ts\"",
    "start": "yarn build:ts && fastify start -l info dist/src/app.js",
    "prod:start": "fastify start -l info dist/src/app.js",
    "build:ts": "tsc",
    "watch:ts": "tsc -w",
    "typecheck": "tsc --noEmit",
    "format": "prettier . --write",
    "dev": "yarn build:ts && concurrently -k -p \"[{name}]\" -n \"TypeScript,App\" -c \"yellow.bold,cyan.bold\" \"yarn:watch:ts\" \"yarn:dev:start\"",
    "dev:start": "fastify start --ignore-watch=.ts$ -w -l info -P dist/src/app.js",
    "generate": "drizzle-kit generate:pg",
    "migrate": "drizzle-kit push:pg"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@fastify/autoload": "^5.0.0",
    "@fastify/jwt": "^8.0.0",
    "@fastify/sensible": "^5.0.0",
    "@fastify/swagger": "^8.13.0",
    "@fastify/swagger-ui": "^2.0.1",
    "@fastify/type-provider-typebox": "^3.5.0",
    "@node-rs/bcrypt": "^1.9.2",
    "@sinclair/typebox": "^0.32.3",
    "date-fns": "^3.3.1",
    "drizzle-orm": "^0.29.1",
    "fastify": "^4.0.0",
    "fastify-cli": "^5.9.0",
    "fastify-plugin": "^4.0.0",
    "getstream": "^8.4.1",
    "jsonwebtoken": "^9.0.2",
    "libphonenumber-js": "^1.10.53",
    "lodash": "^4.17.21",
    "postgres": "^3.4.3",
    "seedrandom": "^3.0.5",
    "twilio": "^4.20.0",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@faker-js/faker": "^8.3.1",
    "@types/lodash": "^4.14.202",
    "@types/node": "^20.4.4",
    "@types/pg": "^8.10.9",
    "@types/seedrandom": "^3.0.8",
    "@types/sinon": "^17.0.2",
    "@types/tap": "^15.0.5",
    "concurrently": "^8.2.2",
    "drizzle-kit": "^0.20.7",
    "fastify-tsconfig": "^2.0.0",
    "prettier": "3.1.1",
    "sinon": "^17.0.1",
    "tap": "^16.1.0",
    "ts-node": "^10.4.0",
    "typescript": "^5.2.2"
  }
}
